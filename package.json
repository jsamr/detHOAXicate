  "scripts": {
    "init": "npm run git-init && npm i && npm i estraverse-fb && npm run install-globals",
    "install-globals": "npm i -g babel-cli@^6.4.0",
    "git-init": "git config commit.template ./.gitmessage && git submodule init && git submodule update && cd node_local_modules/readability && git checkout 71aa562387fa507b0bac30ae7144e1df7ba8a356 && cd ../..",
    "test": "npm run eslint",
    "start": "babel-node server/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "eslint": "node node_modules/eslint/bin/eslint.js .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "@cycle/dom": "^12.2.1",
    "@cycle/http": "^10.2.1",
    "@cycle/isolate": "^1.4.0",
    "@cycle/xstream-run": "^3.1.0",
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cors": "^2.8.0",
    "estraverse-fb": "^1.3.1",
    "font-awesome": "^4.6.3",
    "jsdom": "^9.4.2",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "parse-domain": "^0.2.1",
    "ramda": "^0.22.1",
    "readability": "file:./node_local_modules/readability/",
    "validator": "^5.5.0",
    "xstream": "^6.1.0",
    "yargs": "^6.4.0"
  },
