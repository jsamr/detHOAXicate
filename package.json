    "mocha": "^2.3.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.6.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "engineStrict": {
    "node": ">=4.2.0",
    "npm": ">=3.5.2"
